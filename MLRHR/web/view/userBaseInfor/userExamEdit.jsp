<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html lang="zh-CN">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<title>国土资源科技创新人才工程管理信息系统</title>
<meta name="viewport" content="width=device-width, initial-scale=1.0">
<meta name="description" content="">
<meta name="author" content="">
<style type="text/css">
.td_Left {
	width: 35%;
	text-align: right;
	padding-right: 20px;
}

.td_Right {
	width: 65%;
	text-align: left;
	padding-left: 10px;
}
</style>
<body>
	<form id="userExamForm" class="form-horizontal" style="margin: 0 0 0 0">
		<div style="text-align: center;">
			<div class="control-group">
				<input type="hidden" id="user_exam_id" name="id"
					value="${user_exam.id}" />
				<table width="98%" border="0" cellpadding="2" cellspacing="1"
					align="center" style="margin-top: 8px">
					<tr align="center">
						<td class="td_Left">考试时间：</td>
						<td class="td_Right"><input type="text"
							onfocus="WdatePicker({isShowWeek:true,dateFmt:'yyyy.MM.dd'})"
							id="user_exam_exam_date" value="${user_exam.exam_date}" /></td>
					</tr>
					<tr align="center">
						<td class="td_Left">考试种类：</td>
						<td class="td_Right"><input type="text" id="user_exam_type"
							value="${user_exam.type}" /></td>
					</tr>
					<tr align="center">
						<td class="td_Left">考试科目：</td>
						<td class="td_Right"><input type="text"
							id="user_exam_kemu" value="${user_exam.kemu}" /></td>
					</tr>
					<tr align="center">
						<td class="td_Left">考试成绩：</td>
						<td class="td_Right"><input type="text"
							id="user_exam_chengji" value="${user_exam.chengji}" /></td>
					</tr>
					<tr align="center">
						<td class="td_Left">组织考试单位：</td>
						<td class="td_Right"><input type="text"
							id="user_exam_zuzhizhe" value="${user_exam.zuzhizhe}" /></td>
					</tr>
				</table>
			</div>
		</div>



	</form>
</body>
</html>
